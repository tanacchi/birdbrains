<div class="row">
  <%= form_for(@user, url: users_path(@user)) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.submit "Create my account", class: "btn btn-primary" %>
  <% end %>
  <%= link_to 'Back', users_path %>
</div>
